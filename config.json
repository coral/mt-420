  },
  "lcd": {
    "device": "/dev/cu.usbmodem142444301",
    "baud": 115200
  },
  "fluidsynth": {
    "soundfonts": "files/soundfonts",
    "default": "SC-55.sf2"
  },
  "floppy": {
    "device": "/dev/sdb",
    "mountpoint": "/media/floppy"
  }
