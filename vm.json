{
  "panel": {
    "device": "/dev/ttyACM0",
    "baud": 115200,
    "buttons": [
      {
        "name": "play",
        "message": 32
      },
      {
        "name": "pause",
        "message": 33
      },
      {
        "name": "stop",
        "message": 34
      },
      {
        "name": "menu",
        "message": 35
      },
      {
        "name": "escape",
        "message": 36
      },
      {
        "name": "encoderClick",
        "message": 37
      }
    ],
    "rotary": {
      "encoderLeft": 38,
      "encoderRight": 39
    }
  },
  "lcd": {
    "device": "/dev/ttyACM1",
    "baud": 115200,
    "brightness": 255,
    "contrast": 255,
    "bootdelay": 1
  },
  "fluidsynth": {
    "soundfonts": "files/soundfonts",
    "default": "Roland SC-55.sf2",
    "gain": 0.5
  },
  "floppy": {
    "device": "/dev/sdb",
    "mountpoint": "/media/floppy"
  }
}
